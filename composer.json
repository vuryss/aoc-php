{
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-bcmath": "*",
        "ext-ds": "*",
        "ext-gmp": "*",
        "symfony/console": "7.1.*",
        "symfony/flex": "^2.4.7",
        "symfony/framework-bundle": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/yaml": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "twig/extra-bundle": "^3.13",
        "twig/twig": "^3.14",
        "symfony/dom-crawler": "7.1.*",
        "symfony/css-selector": "7.1.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "vimeo/psalm": "^5.26"
    }
}
